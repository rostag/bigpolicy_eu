{
  "name": "bp-project",
  "version": "1.1.161",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node start",
    "server:start:dev": "cd nestjsserver && npm run start:dev",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "serve": "ng build --watch true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "db": "cd ../ && mongod --dbpath=data/db --port 27027",
    "db-auth": "cd ../ && mongod --auth --dbpath=data/db --port 27027",
    "server": "node start",
    "client": "ng serve --port=5000",
    "d": "npm run deploy",
    "deploy": "npm run deploy-fbs",
    "deploy-fbs": "npm version patch && npm install && npm run build-prod && rsync -a dist/* fbs/dist && cd fbs && firebase deploy",
    "deploy-fbs-and-code-update": "npm version patch && npm install && npm run build-prod && git push origin && rsync -a dist/* fbs/dist && cd fbs && firebase deploy --only hosting",
    "deploy-fbs-hosting": "npm version patch && npm install && npm run build-prod && rsync -a dist/* fbs/dist && cd fbs && firebase deploy --only hosting",
    "deploy-fbs-functions": "npm version patch && cd fbs && firebase deploy --only functions",
    "dump-local-db": "cd ../ && mongodump --port 27027 --archive=local-bp-db-dump.zip --db bp --verbose --gzip"
  },
  "main": "start.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.3",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "14.2.3",
    "@angular/compiler": "14.2.3",
    "@angular/compiler-cli": "14.2.3",
    "@angular/core": "14.2.3",
    "@angular/fire": "^7.4.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "14.2.3",
    "@angular/localize": "^14.2.3",
    "@angular/material": "14.2.2",
    "@angular/platform-browser": "14.2.3",
    "@angular/platform-browser-dynamic": "14.2.3",
    "@angular/platform-server": "14.2.3",
    "@angular/router": "14.2.3",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngrx/effects": "^14.3.1",
    "@ngrx/store": "^14.3.1",
    "@ngrx/store-devtools": "^14.3.1",
    "@ngu/carousel": "^6.0.1",
    "auth0-js": "^9.19.1",
    "auth0-lock": "^11.34.0",
    "core-js": "2.6.3",
    "firebase": "^9.10.0",
    "googleapis": ">=39.1.0",
    "hammerjs": "^2.0.8",
    "intl": "1.2.5",
    "ng2-pagination": "2.0.2",
    "node-pre-gyp": "^0.13.0",
    "node-sass": "^7.0.3",
    "python": "0.0.4",
    "rxjs": "^6.6.7",
    "sass-loader": "^7.1.0",
    "tslib": "^2.0.0",
    "typescript": "4.6.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "14.2.3",
    "@types/gapi.auth2": "0.0.48",
    "@types/google-drive-realtime-api": "0.0.33",
    "@types/jasmine": "~3.6.0",
    "@types/node": "10.12.21",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonfile": "5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "7.0.0",
    "ts-node": "8.0.2",
    "tslint": "6.1.3"
  }
}
